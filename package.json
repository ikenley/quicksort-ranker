{
  "name": "quicksort-ranker",
  "version": "1.0.0",
  "description": "Web UI for rankings lists using the quicksort algorithm",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start-prod": "node dist/index.js",
    "start": "nodemon",
    "pretest": "npm run clean",
    "test": "jest --detectOpenHandles",
    "pretest-ci": "npm run clean",
    "test-ci": "jest --coverage",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf dist build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikenley/quicksort-ranker.git"
  },
  "keywords": [
    "quicksort"
  ],
  "author": "ikenley",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/ikenley/quicksort-ranker/issues"
  },
  "homepage": "https://github.com/ikenley/quicksort-ranker#readme",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "eslint": "^8.34.0",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
